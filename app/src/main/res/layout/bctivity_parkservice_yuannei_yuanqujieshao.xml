<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".perkservice.parkservice_yuannei_yuanqujieshao">

    <Toolbar
        android:id="@+id/yuannei_yuanqujieshao_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?android:actionBarSize"
        android:background="?attr/colorPrimary"
        android:navigationIcon="@drawable/back_button"
        android:title=""
        android:titleTextColor="@android:color/white">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="@string/yuanqujieshao"
            android:textColor="@android:color/white"
            android:textSize="20sp" />
    </Toolbar>

    <WebView
        android:id="@+id/yuanqujieshao_webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </WebView>


</LinearLayout>