<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/first_gongshangzhuce"
    android:layout_width="match_parent"
    android:layout_height="135dp"
    android:layout_marginStart="17dp"
    android:layout_marginTop="11dp"
    android:layout_marginEnd="17dp"
    android:background="@drawable/qiyefuwu_gongshangzhuce_icon">


    <LinearLayout
        android:layout_width="200dp"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:orientation="vertical">

        <TextView
            android:id="@+id/qiyefuwu_qid_text"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginEnd="24dp"
            android:layout_weight="2"
            android:gravity="center_vertical"
            android:text="XXX工商注册"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/qiyefuwu_average_cost_text"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginEnd="24dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:text="平均费用30元" />

        <TextView
            android:id="@+id/qiyefuwu_park_use_rate_text"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginEnd="24dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:text="园区使用率30%" />

        <TextView
            android:id="@+id/qiyefuwu_average_time_text"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginBottom="17dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:text="平均申请时间30天" />


    </LinearLayout>


</RelativeLayout>